<%- include('partials/adminheader') %>
<div class="container" style="background-color:#D3D3D3;
  position: relative;
        
        top: 100px;
    width: 60%;
  padding: 25px;">




  <form method="post" action="/profile_edit" enctype="multipart/form-data">





    <div class="form-group">


      <div class="form-group">
        <label for="dateofbirth">DOB</label>
        <input type="date" value="<%=user.dateofbirth%>" name="dateofbirth" class="form-control" id="dateofbirth"
          required />
      </div>
      <div class="form-group">
        <label for="bio">BIO</label>
        <input type="text" value="<%=user.bio%>" name="bio" class="form-control" id="email" placeholder="Enter bio"
          required />
      </div>
      <div class="form-group">
        <label for="upload">upload</label>
        <input type="file" name="upload" class="form-control" id="upload">
      </div>
      <button type="submit" class="btn btn-primary">edit profile</button>
  </form>

</div>
</body>

</html>